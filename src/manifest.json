{
	"manifest_version": 2,
	"name": "watch-from-mal-chrome-extension",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": ["https://myanimelist.net/anime/*/*"],
			"js": ["content.bundle.js"],
			"run_at": "document_end"
		}
	],

	"permissions": [
		"tabs",
		"http://gogoanime.io/",
		"https://gogoanime.io/"
	]

}
